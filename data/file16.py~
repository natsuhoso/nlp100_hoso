import sys
import numpy as np

file = sys.argv[1]
n = int(sys.argv[2])

with open(file) as r:
    text = r.read()

rows = np.array(text.split('\n'))
num = 0
for i in np.array_split(rows,n):
    with open(f'data/dir16_b/file{num}.txt', mode = 'w') as w:
        w.write("\n".join(i))
    num = num + 1
